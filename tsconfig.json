{
  "compilerOptions": {
    "types": [
      "vite/client",
      "@types/node"
    ],
    "experimentalDecorators": true,
    "target": "ESNext",
    "lib": [
      "ESNext",
      "dom",
      "WebWorker"
    ],
    "useDefineForClassFields": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitThis": false,
    "baseUrl": ".",
    "paths": {
      "@*":["src/pages/video/music/synthwave/*"],
      "~/*": ["src/*"],
      "~app/*": ["config/app/*"],
      "~uno/*": ["config/uno/*"],
      "~ops/*": ["ops/*"],
      "~root/*": ["*"]
    }
  },
  "include": [
    "**/*.json",
    "**/*.ts",
    "**/*.js",
    "**/*.jsx",
    "**/*.tsx" ]
}
