  "name": "trill-pics",
  "version": "1.0.0",
  "description": "Picture gallery",
  "main": "src/main.tsx",
  "types": "src/index.d.ts",
  "scripts": {
    "lint": "tsc --build . & eslint . --fix --ext ts,tsx",
    "api": "tsx watch ./src/server/api",
    "dev:api": "npm run api",
    "dev:client": "vite",
    "dev": "run-p dev:* --print-label",
    "build": "vite build",
    "preview:api": "npm run api",
    "preview:client": "vite preview",
    "preview": "_IS_LOCAL=true && run-p preview:* --print-label",
    "start": "npm run api && npm run preview",
    "l": "npm run lint",
    "dd": "docker build . -t \"sample-project:v1.0\"",
    "dr": "docker run -d -p 8080:8080 sample-project:v1.0",
    "f": "prettier .",
    "@remotion": "npx update-by-scope @remotion npm install",
    "@r": "npm run @remotion",
    "@brysonandrew": "npx update-by-scope @brysonandrew npm install",
    "erase": "esrun ops/eraser",
    "@b": "npm run erase && npm run @brysonandrew && npm run lint",
    "ops/collection": "esrun ops/collection",
    "ops/routes": "esrun ops/routes",
    "test/screens": "esrun test/screens",
    "ops/favicons": "esrun ops/favicons",
    "ds": "gh workflow run && gh run list --workflow=studio.yml",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
